Code 1
//interface
//an interface in a java is an abstract type that defines a set of methods a class must implement
//Multiple inheritance using interfaces
interface tv{
void watchingTv();
}
interface food {
void eatingFood();
}
class Student{
public void watchingTv(){
System.out.println("watching tv");
 }
public void eatingFood(){
System.out.println("eatingFood");
 }
}
public class Main{
public static void main(String[]args){
Student s=new Student();
s.watchingTv();
s.eatingFood();
}
}
//Student class inherit multiple interfaces
//Code 2
//Inheriting the constructor
//constructor can be inherited using super keyword

class Person{
Person(String name){
    System.out.println("Person Name:"+name);
 }
} 
class CollegeStudent extends Person{
    CollegeStudent(String name){
        super(name);//parent class constructor called
        System.out.println("College student constructor called");//child class constructor called
    }
}
public class Main
{
    public static void main (String[] args) {
        CollegeStudent std = new CollegeStudent("Deeksha");
    }
}
//Code 3
//Inheriting attributes

class University{
    String universityName="IILM University";
}
class College extends University{
    void displayUniversityName(){
        System.out.println("University:" + universityName);
    }
}
public class Main
{
public static void main (String[] args) {
   College c=   new College();
   c.displayUniversityName();
 }
}
//Code 4
// Inheriting methods 

class Parent {
    void pockeyMoney() {
        System.out.println("Pockey Money credit");
    }
}
class child extends Parent{
    void freelancing() {
        System.out.println("Kudh ka paisa");
    }
}
public class Main
{
	public static void main(String[] args) {
		child ch = new child();
		ch.pockeyMoney();
		ch.freelancing();
	}
}
//Code 5
// Method overriding

class CollegeStudent {
    void task() {
        System.out.println("Eating food");
    }
}
class Student extends CollegeStudent {
    @Override
    void task() {
        System.out.println("Paani pee raha hai");
    }
}

public class Main
{
	public static void main(String[] args) {
		CollegeStudent obj = new Student();
		obj.task();
	}
}
